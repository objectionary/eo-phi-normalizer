            φ ↦ ξ.cosine.eq(0).if(Φ̇.nan, ξ.ρ.sin.div(ξ.cosine))
          ⟧,
          ctan ↦ ⟦
            sine ↦ Φ̇.dataized(ξ.ρ.sin).as-bytes,
            φ ↦ ξ.sine.eq(0).if(Φ̇.nan, ξ.ρ.cos.div(ξ.sine))
          ⟧
        ⟧,
        λ ⤍ Package
      ⟧,
