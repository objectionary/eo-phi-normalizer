{⟦
  org ↦ ⟦
    eolang ↦ ⟦
      math ↦ ⟦
        angle(value) ↦ ⟦
          φ ↦ ξ.value,
          in-degrees ↦ ξ.ρ.angle(ξ.ρ.times(180).div(Φ̇.math.pi)),
          in-radians ↦ ξ.ρ.angle(ξ.times(Φ̇.math.pi).div(180)),
          sin ↦ ⟦
            λ ⤍ Lorg_eolang_math_angle_sin
          ⟧,
